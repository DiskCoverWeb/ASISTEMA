Private Sub Command2_Click()
   sSQL = "SELECT F.T,F.Fecha,F.Saldo_MN,F.Factura " _
        & "FROM Facturas As F,Clientes As C " _
        & "WHERE F.Fecha <= #" & FechaFin & "# " _
        & "AND F.Item = '" & NumEmpresa & "' " _
        & "AND F.Periodo = '" & Periodo_Contable & "' " _
        & "AND F.CodigoC = C.Codigo " _
        & "AND F.TC <> 'C' " _
        & "AND F.TC <> 'P' "
   'If Option2.Value Then
      sSQL = sSQL & "AND F.CodigoC = '" & CodigoCli & "' "
   'Else
'      If LstCampos.Text = "Grupo" Then sSQL = sSQL & "AND C.Grupo = '" & TxtCIRUC.Text & "' "
   'End If
   sSQL = sSQL & "AND F.T = '" & Pendiente & "' " _
        & "ORDER BY F.CodigoC,F.Fecha,F.Factura "
SelectAdodc AdoFactura, sSQL
End Sub

Private Sub Form_Activate()
  sSQL = "SELECT * " _
       & "FROM Clientes " _
       & "WHERE Cliente <> '.' " _
       & "AND FA = " & Val(adTrue) & " " _
       & "ORDER BY Cliente "
  SelectDBCombo DCCliente, AdoListCtas, sSQL, "Cliente"
  RatonNormal
End Sub

Private Sub Form_Load()
  ConectarAdodc AdoFactura
  ConectarAdodc AdoListCtas
  ConectarAdodc AdoPendiente
End Sub
