  DataCajaCred.DatabaseName = RutaEmpresa & "\CAJACRED.MDB"
End Sub

Private Sub List2_DblClick()
  SiguienteControl
End Sub

Private Sub List2_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeyDelete Then
     Mensajes = "Esta seguro de Eliminar la Tabla " & List2.Text & "]"
     Titulo = "Pregunta de Eliminacion"
     If BoxMensaje = 6 Then
        sSQL = "DROP TABLE " & List2.Text & " "
        DataContabil1.Database.Execute sSQL
        DataContabil1.Refresh
        List2.Clear
        For Each Td In DataContabil1.Database.TableDefs
         If UCase(Mid(Td.Name, 1, 4)) <> "MSYS" Then List2.AddItem Td.Name
        Next
     End If
  End If
End Sub

Private Sub List2_LostFocus()
  sSQL = "SELECT * FROM " & List2.Text & " "
  SelectAdodc DataContabil1, sSQL
End Sub

Private Sub MBoxFechaC_GotFocus()
  MarcarTexto MBoxFechaC
End Sub

Private Sub MBoxFechaC_LostFocus()
  FechaValida MBoxFechaC
End Sub

Private Sub MBoxFechaF_LostFocus()
  FechaValida MBoxFechaF, False
End Sub

Private Sub MBoxFechaI_LostFocus()
   FechaValida MBoxFechaI, False
End Sub

Private Sub TextTP_GotFocus()
  TextTP.Text = ""
End Sub

Private Sub TextTP_LostFocus()
  TextTP.Text = UCase(TextTP.Text)
End Sub

