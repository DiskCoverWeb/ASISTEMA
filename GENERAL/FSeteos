End Sub

Private Sub DBGrid4_BeforeDelete(Cancel As Integer)
  Cancel = DeleteSiNo(DataCtasProc)
End Sub

Private Sub Dir1_Change()
  File1.FileName = Dir1.Path & "\*.MDB"
End Sub

Private Sub File3_LostFocus()
  DataContabil1.DatabaseName = Dir3.Path & "\" & File3.FileName
  DataContabil1.Refresh
End Sub

Private Sub Form_Activate()
  IniciarAsientosDataDe DataAsientos, DataSubCtas, DataBanco, DataRet, DataIngArt
  For I = 0 To 5
      CheckNivel(I).Value = Nivel_N(I)
  Next I
  CheckSupervisor.Value = CSupervisor
  sSQL = "SELECT Nombre_Completo,Clave " _
       & "FROM Accesos " _
       & "WHERE Mid(Codigo,1,6) = 'ACCESO' " _
       & "ORDER BY Codigo "
  'SelectDBGrid DBGrid6, DataSupervisor, sSQL
  sSQL = "SELECT * FROM Codigos ORDER BY Concepto "
  'SelectDBGrid DBGrid3, DataCodigos, sSQL
  sSQL = "SELECT * FROM CtasProceso ORDER BY Codigo,Cuenta "
  'SelectDBGrid DBGrid4, DataCtasProc, sSQL
  sSQL = "SELECT * FROM Niveles ORDER BY Nivel "
  'SelectDBGrid DBGrid5, DataSubCtas1, sSQL
' Lista de Usuarios en la red
  sSQL = "SELECT (Codigo & Space(4) & Nombre_Completo) As NombUsuar " _
       & "FROM Accesos " _
       & "WHERE Mid(Codigo,1,6) <> 'ACCESO' " _
       & "ORDER BY Codigo,Nombre_Completo "
  SelectDBCombo DBCUsuario, DataEmpresa, sSQL, "NombUsuar", False
  Select Case UCase(App.EXEName)
    Case "FAMENVIO"
         Cadena = "CONFIRMACION DE PAGOS" & vbCrLf & Chr(10) _
                & "ANULAR"
         TextNiveles(0).Text = Cadena
         Cadena = "OBLIGACIONES POR PAGAR"
         TextNiveles(1).Text = Cadena
         Cadena = "FLUJO DE CAJA" & vbCrLf & Chr(10) _
                & "TRANSFERENCIAS" & vbCrLf & Chr(10) _
                & "RECEPCION ENVIOS"
         TextNiveles(2).Text = Cadena
         Cadena = "ACTUALIZACION DE REMITENTES"
         TextNiveles(3).Text = Cadena
         Cadena = "CORRESPONSALES" & vbCrLf & Chr(10) _
                & "TRANSFERENCIAS"
         TextNiveles(4).Text = Cadena
         TextNiveles(5).Text = "RECIBOS DE PAGO"
    Case "CONTABIL"
         Cadena = "MANTENIMIENTO" & vbCrLf & Chr(10) _
                & "INGRESO DEL CATALOGO DE CUENTAS" & vbCrLf & Chr(10) _
                & "PROCESAR INGRESOS - EGRESOS - DIARIOS" & vbCrLf & Chr(10) _
                & "MAYORIZAR - PROCESAR BALANCES DE COMPROBACION" & vbCrLf & Chr(10) _
                & "LISTAR BALANCES PRINCIPALES" & vbCrLf & Chr(10) _
                & "LISTAR COMPROBANTES" & vbCrLf & Chr(10) _
                & "ANULAR COMPROBANTES"
         TextNiveles(0).Text = Cadena
         Cadena = "INGRESO DEL CATALOGO DE CUENTAS" & vbCrLf & Chr(10) _
                & "PROCESAR INGRESOS EGRESOS DIARIOS" & vbCrLf & Chr(10) _
                & "MAYORIZAR - PROCESAR BALANCE DE COMPROBACION" & vbCrLf & Chr(10) _
                & "LISTAR BALANCES PRINCIPALES" & vbCrLf & Chr(10) _
                & "LISTAR COMPROBANTES"
         TextNiveles(1).Text = Cadena
         Cadena = "INGRESO DEL CATALOGO DE CUENTAS" & vbCrLf & Chr(10) _
                & "PROCESAR INGRESOS - EGRESOS - DIARIOS" & vbCrLf & Chr(10) _
                & "LISTAR COMPROBANTES"
         TextNiveles(2).Text = Cadena
         Cadena = "PROCESAR INGRESOS - EGRESOS - DIARIOS" & vbCrLf & Chr(10) _
                & "LISTAR COMPROBANTES"
         TextNiveles(3).Text = Cadena
         Cadena = ""
         TextNiveles(4).Text = Cadena
         TextNiveles(5).Text = Cadena
    Case "CAJACRED"
         Cadena = "ADREDITAR INTERESES" & vbCrLf & Chr(10) _
                & "LISTAR CUENTAS" & vbCrLf & Chr(10) _
                & "VER MOVIMIENTO DE CUENTAS" & vbCrLf & Chr(10) _
                & "HISTORIAL DE CUENTAS" & vbCrLf & Chr(10) _
                & "GRABAR CIERRE DEL DIA" & vbCrLf & Chr(10) _
                & "VER SALDO DE INTERES ACUMULADOS POR PAGAR DIAS ANTERIORES" & vbCrLf & Chr(10) _
                & "CAMBIAR DIAS DE EFECTIVIZACION" & vbCrLf & Chr(10) _
                & "PROTESTAR CHEQUES" & vbCrLf & Chr(10) _
                & "EFECTIVIZAR CHEQUES"
         TextNiveles(0).Text = Cadena
         Cadena = "DEPOSITOS Y RETIROS" & vbCrLf & Chr(10) _
                & "REALIZAR INGRESOS Y EGRESOS DE CAJA" & vbCrLf & Chr(10) _
                & "DEPOSITOS AL BANCO" & vbCrLf & Chr(10) _
                & "IMPRIMIR FLUJO DE CAJA Y LIBRETAS" & vbCrLf & Chr(10) _
                & "HISTORIAL DE CUENTAS"
         TextNiveles(1).Text = Cadena
         Cadena = "APERTURAS" & vbCrLf & Chr(10) _
                & "TRANSACCIONES DE CREDITO" & vbCrLf & Chr(10) _
                & "ENCAJES" & vbCrLf & Chr(10) _
                & "LISTAR CUENTAS" & vbCrLf & Chr(10) _
                & "VER MOVIMIENTO DE CUENTAS" & vbCrLf & Chr(10) _
                & "IMPRIMIR MOVIMIENTO DE CUENTAS" & vbCrLf & Chr(10) _
                & "IMPRIMIR CREDITO" & vbCrLf & Chr(10) _
                & "HISTORIAL DE CUENTAS"
         TextNiveles(2).Text = Cadena
         Cadena = "TRANSACCIONES DE CREDITO" & vbCrLf & Chr(10) _
                & "IMPRIMIR REPORTE DE NOTAS DE DEBITO Y CREDTITO"
         TextNiveles(3).Text = Cadena
         Cadena = ""
         TextNiveles(4).Text = "MANTENIMIENTO"
         TextNiveles(5).Text = Cadena
  End Select
  sSQL = "SELECT * FROM Empresas ORDER BY Empresa "
  SelectDBCombo DBCListEmp, DataListEmp, sSQL, "Empresa", False
  RatonNormal
End Sub

Private Sub Form_Load()
  CentrarForm FSeteos
' Direccion de Bases de la empresa
  Dir1.Path = RutaEmpresa
  File1.FileName = Dir1.Path & "\*.MDB"
  Dir2.Path = RutaEmpresa
  File2.FileName = Dir2.Path & "\*.MDB"
  Dir3.Path = RutaEmpresa
  File3.FileName = Dir2.Path & "\*.MDB"
' Direccion del encabezado de las empresas
  DataListEmp.DatabaseName = RutaSistema & "\EMPRESAS.MDB"
  DataBuscEmp.DatabaseName = RutaSistema & "\EMPRESAS.MDB"
  DataEmpresa.DatabaseName = RutaSistema & "\EMPRESAS.MDB"
  DataEmpresa1.DatabaseName = RutaSistema & "\EMPRESAS.MDB"
  DataSupervisor.DatabaseName = RutaSistema & "\EMPRESAS.MDB"
' Direccion de Contabilidad e Inventario
  DataRet.DatabaseName = RutaEmpresa & "\CONTABIL.MDB"
  DataBanco.DatabaseName = RutaEmpresa & "\CONTABIL.MDB"
  DataConta.DatabaseName = RutaEmpresa & "\CONTABIL.MDB"
  DataIngArt.DatabaseName = RutaEmpresa & "\CONTABIL.MDB"
  DataDetRet.DatabaseName = RutaEmpresa & "\CONTABIL.MDB"
  DataSubCtas.DatabaseName = RutaEmpresa & "\CONTABIL.MDB"
  DataSaldoSC.DatabaseName = RutaEmpresa & "\CONTABIL.MDB"
  DataAsientos.DatabaseName = RutaEmpresa & "\CONTABIL.MDB"
  DataContabil.DatabaseName = RutaEmpresa & "\CONTABIL.MDB"
  DataSubCtaDet.DatabaseName = RutaEmpresa & "\CONTABIL.MDB"
  DataAsientBanco.DatabaseName = RutaEmpresa & "\CONTABIL.MDB"
' Direccion de direccion de Facturacion
  DataProducc.DatabaseName = RutaEmpresa & "\PRODUCC.MDB"
  'DataFactTurs.DatabaseName = RutaEmpresa & "\PRODUCC.MDB"
  DataSubCtas1.DatabaseName = RutaEmpresa & "\PRODUCC.MDB"
  DataFacturas.DatabaseName = RutaEmpresa & "\PRODUCC.MDB"
' Direccion de Seteos Principales
  DataCodigos.DatabaseName = RutaEmpresa & "\SETEOS.MDB"
  DataCtasProc.DatabaseName = RutaEmpresa & "\SETEOS.MDB"
' Direccion de Base de Famiremesas
  DataEnvios.DatabaseName = RutaEmpresa & "\ENVIOS.MDB"
  DataResult.DatabaseName = RutaEmpresa & "\ENVIOS.MDB"
  DataCorreos.DatabaseName = RutaEmpresa & "\ENVIOS.MDB"
' Direccion de Bases de Cooperativas
