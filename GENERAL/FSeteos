Attribute VB_Name = "FSeteos"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
'Despues lo arreglare
Private Sub CheckSupervisor_Click()
If CheckSupervisor.Value Then
   For I = 0 To 5
       CheckNivel(I).Value = False
   Next I
End If
End Sub

Private Sub Command1_Click()
  Cadena = DBGrid2.Columns(I).DataField
  sSQL = "SELECT * FROM " & List2.Text & " " _
       & "ORDER BY " & Cadena & " ASC "
  SelectAdodcdc DataContabil1, sSQL
End Sub

Private Sub Command10_Click()
  sSQL = "SELECT T.Fecha,T.TP,T.Numero,C.Codigo,C.Cuenta,T.Debe,T.Haber,T.Debe_ME,T.Haber_ME " _
       & "FROM Transacciones As T,Catalogo As C " _
       & "WHERE C.DG = 'G' " _
       & "AND C.Codigo = T.Cta "
  SelectDBGrid DBGrid1, DataContabil, sSQL
End Sub

Private Sub Command11_Click()
   RatonReloj
   TextoValido TextClave
   Codigo = SinEspaciosIzq(DBCUsuario.Text)
   sSQL = "SELECT * FROM " & UCase(App.EXEName) & " " _
        & "WHERE Codigo = '" & Codigo & "' "
   SelectAdodcdc DataEmpresa1, sSQL, False
   With DataEmpresa1.Recordset
        If .RecordCount > 0 Then
           .Edit
        Else
           .AddNew
           .Fields("Codigo") = Codigo
        End If
       .Fields("Nivel_1") = CheckNivel(0).Value
       .Fields("Nivel_2") = CheckNivel(1).Value
       .Fields("Nivel_3") = CheckNivel(2).Value
       .Fields("Nivel_4") = CheckNivel(3).Value
       .Fields("Nivel_5") = CheckNivel(4).Value
       .Fields("Nivel_6") = CheckNivel(5).Value
       .Fields("Supervisor") = CheckSupervisor.Value
       .Update
   End With
   sSQL = "SELECT * FROM Accesos " _
        & "WHERE Codigo = '" & Codigo & "' "
   SelectAdodcdc DataEmpresa1, sSQL, False
   With DataEmpresa1.Recordset
    If .RecordCount > 0 Then
       .Edit
       .Fields("TODOS") = CheckSupervisor.Value
       .Fields("Clave") = TextClave.Text
       .Update
    End If
   End With
   RatonNormal
   MsgBox "Niveles de seguridad grabado"
End Sub

Private Sub Command12_Click()
 Unload FSeteos
End Sub

Private Sub Command13_Click()
  sSQL = "SELECT * FROM TC "
  SelectAdodc DataAuxCaja, sSQL
  sSQL = "SELECT Cuenta_No,Fecha,TP,ABS(Debitos-Creditos) AS Valor,Cheque,Hora,Usuario " _
       & "FROM Trans_Cajas " _
       & "WHERE TP<>'BOVE' " _
       & "ORDER BY Cuenta_No,Fecha "
  SelectAdodc DataCajaCred, sSQL
  With DataCajaCred.Recordset
       RatonReloj
       Contador = 0
       Do While Not .EOF
          FSeteos.Caption = "Contador = " & Contador & "/" & .RecordCount
          Cuenta_No = .Fields("Cuenta_No")
          TipoDoc = .Fields("TP")
          MiFecha = .Fields("Fecha")
          ValorTotal = Round(.Fields("Valor"))
          Cadena = .Fields("Usuario")
          Cadena1 = .Fields("Hora")
          Contador = Contador + 1
          sSQL = "SELECT Cuenta_No,Fecha,TP,ABS(Debitos-Creditos) AS Valor,Cheque,Hora,Usuario " _
               & "FROM Trans_Libretas " _
               & "WHERE Cuenta_No='" & Cuenta_No & "' " _
               & "AND Fecha=#" & BuscarFecha(MiFecha) & "# " _
               & "AND TP='" & TipoDoc & "' " _
               & "AND ABS(Debitos-Creditos)=" & ValorTotal & " "
          SelectAdodc DataCaja, sSQL
          If DataCaja.Recordset.RecordCount <= 0 Then
             DataAuxCaja.Recordset.AddNew
             DataAuxCaja.Recordset.Fields("Fecha") = MiFecha
             DataAuxCaja.Recordset.Fields("Cuenta_No") = Cuenta_No
             DataAuxCaja.Recordset.Fields("TP") = TipoDoc
             DataAuxCaja.Recordset.Fields("Valor") = ValorTotal
             DataAuxCaja.Recordset.Fields("Usuario") = Cadena
             DataAuxCaja.Recordset.Fields("Hora") = Cadena1
             DataAuxCaja.Recordset.Update
          End If
         .MoveNext
       Loop
  End With
  RatonNormal
End Sub

Private Sub Command14_Click()
   Titulo = "Pregunta de Actualizacion"
   Mensajes = "Seguro de actualizar la base" & vbCrLf _
            & NumEmpresa & " - " & Empresa
   If BoxMensaje = 6 Then
      RatonReloj
      sSQL = "SELECT * FROM Transacciones " _
           & "WHERE ID = 0 " _
           & "ORDER BY Fecha,TP,Numero "
      SelectAdodc DataContabil, sSQL
      With DataContabil.Recordset
           ID_Trans = 0
           Do While Not .EOF
             .Edit
             .Fields("ID") = ID_Trans
             .Update
             .MoveNext
              ID_Trans = ID_Trans + 1
           Loop
      End With
      sSQL = "SELECT * FROM TransaccionesSC " _
           & "WHERE ID = 0 " _
           & "ORDER BY Fecha,TP,Numero "
      SelectAdodc DataContabil, sSQL
      With DataContabil.Recordset
           ID_Trans = 0
           Do While Not .EOF
             .Edit
             .Fields("ID") = ID_Trans
             .Update
             .MoveNext
              FSeteos.Caption = "Actualizando Numero de Transaccion No. " & ID_Trans
              ID_Trans = ID_Trans + 1
           Loop
      End With
      sSQL = "UPDATE Transacciones " _
           & "SET Item = " & NumEmpresa & " "
      UpdateData DataContabil, sSQL
      sSQL = "UPDATE TransaccionesSC " _
           & "SET Item = " & NumEmpresa & " "
      UpdateData DataContabil, sSQL
      sSQL = "UPDATE TransaccionesGC " _
           & "SET Item = " & NumEmpresa & " "
      UpdateData DataContabil, sSQL
      sSQL = "UPDATE Comprobantes " _
           & "SET Item = " & NumEmpresa & " "
      UpdateData DataContabil, sSQL
      sSQL = "UPDATE Bancos " _
           & "SET Item = " & NumEmpresa & " "
      UpdateData DataContabil, sSQL
      sSQL = "UPDATE Retenciones " _
           & "SET Item = " & NumEmpresa & " "
      UpdateData DataContabil, sSQL
      sSQL = "UPDATE Kardex " _
           & "SET Item = " & NumEmpresa & " "
      UpdateData DataContabil, sSQL
      RatonNormal
   End If
   FSeteos.Caption = "SETEOS PRINCIPALES Y MANTENIMIENTO"
   MsgBox "Proceso Terminado"
End Sub

Private Sub Command15_Click()
   Codigo = InputBox("Ingresar Texto", "Datos")
   Codigo1 = InputBox("Ingresar Linea de Produccion:", "Datos")
   If Codigo <> "" And Codigo1 <> "" Then
      sSQL = "UPDATE Contratos_Meses " _
           & "SET Codigo = '" & UCase(Codigo1) & "' " _
           & "WHERE Mid(Contrato_No,1," & Len(Codigo) & ") = '" & UCase(Codigo) & "' "
      UpdateData DataFacturas, sSQL
   End If
End Sub

Private Sub Command16_Click()
  FechaValida MBoxFechaC
  FechaFin = BuscarFecha(MBoxFechaC.Text)
  FSeteos.Caption = "Eliminando Flujo de Caja"
  sSQL = "DELETE * FROM Flujo_Caja " _
       & "WHERE Fecha <= #" & FechaFin & "# "
  DeleteData DataEnvios, sSQL
  FSeteos.Caption = "Eliminando Correos Cancelados"
  sSQL = "DELETE * FROM Correos " _
       & "WHERE Fecha_P <= #" & FechaFin & "# " _
       & "AND T = 'C' "
  DeleteData DataEnvios, sSQL
  FSeteos.Caption = "Eliminando Correos Anulados"
  sSQL = "DELETE * FROM Correos " _
       & "WHERE Fecha_P <= #" & FechaFin & "# " _
       & "AND T = 'A' "
  DeleteData DataEnvios, sSQL
  sSQL = "SELECT * FROM Correos " _
       & "ORDER BY Cod_B "
  SelectDBGrid DBGEnvios, DataEnvios, sSQL
  With DataEnvios.Recordset
   If .RecordCount > 0 Then
      .MoveLast
       Contador = .Fields("Cod_B")
       DBGEnvios.Visible = False
       I = 0
      .MoveFirst
       Do While Not .EOF
          J = .Fields("Cod_B")
          FSeteos.Caption = "(" & Contador & ") Eliminando Beneficiarios: " & I & " - " & J
          sSQL = "DELETE * FROM Beneficiarios " _
               & "WHERE Codigo_B > " & I & " " _
               & "AND Codigo_B < " & J & " "
          DeleteData DataResult, sSQL
          I = J
         .MoveNext
       Loop
   End If
  End With
  DBGEnvios.Visible = True
  sSQL = "SELECT * FROM Correos " _
       & "ORDER BY Cod_R "
  SelectDBGrid DBGEnvios, DataEnvios, sSQL
  With DataEnvios.Recordset
   If .RecordCount > 0 Then
      .MoveLast
       Contador = .Fields("Cod_R")
       DBGEnvios.Visible = False
       I = 0
      .MoveFirst
       Do While Not .EOF
          J = .Fields("Cod_R")
          FSeteos.Caption = "(" & Contador & ") Eliminando Remitentes:  " & I & " - " & J
          sSQL = "DELETE * FROM Remitentes " _
               & "WHERE Codigo_R > " & I & " " _
               & "AND Codigo_R < " & J & " "
          DeleteData DataResult, sSQL
          I = J
         .MoveNext
       Loop
   End If
  End With
  sSQL = "UPDATE Beneficiarios " _
       & "SET Beneficiario = Trim(Beneficiario) "
  UpdateData DataResult, sSQL
  sSQL = "UPDATE Remitentes " _
       & "SET Remitente = Trim(Remitente) "
  UpdateData DataResult, sSQL
  DBGEnvios.Visible = True
End Sub

Private Sub Command17_Click()
Dim No_Exit As Boolean
   FechaValida MBoxFechaC
   NombreArchivo = RutaSistema & "\GIROS_INCOMPLETOS.TXT"
   Open NombreArchivo For Output As #1 ' Abre el archivo.
   Print #1, "Giros Incompletos: "
   sSQL = "SELECT Correos.* " _
        & "FROM Correos,Beneficiarios,Remitentes " _
        & "WHERE Correos.Cod_B = Beneficiarios.Codigo_B " _
        & "AND Correos.Cod_R = Remitentes.Codigo_R " _
        & "ORDER BY Correos.Envio_No,Correos.Fecha "
   SelectAdodc DataResult, sSQL
           
   sSQL = "SELECT * " _
        & "FROM Correos " _
        & "WHERE Fecha>=#" & BuscarFecha(MBoxFechaC.Text) & "# " _
        & "ORDER BY Envio_No,Fecha "
   SelectAdodc DataEnvios, sSQL
   With DataEnvios.Recordset
    If .RecordCount > 0 Then
        Do While Not .EOF
           No_Exit = False
           Codigo = .Fields("Envio_No")
           I = .Fields("Cod_B")
           J = .Fields("Cod_R")
           FSeteos.Caption = Codigo & " - " & .Fields("Fecha")
           If DataResult.Recordset.RecordCount > 0 Then
              DataResult.Recordset.MoveFirst
              Do While Not DataResult.Recordset.EOF And No_Exit = False
                 If Codigo = DataResult.Recordset.Fields("Envio_No") Then No_Exit = True
                 DataResult.Recordset.MoveNext
              Loop
           End If
           If No_Exit Then
              Cadena = "Giro No. " & Codigo & " Benef.: " & I & ", Remit.: " & J
              Print #1, Cadena
           End If
          .MoveNext
        Loop
     End If
   End With
   Close #1
   Unload FSeteos
End Sub

Private Sub Command18_Click()
  For I = 0 To List3.ListCount - 1
     List3.ListIndex = I
     sSQL = "DROP TABLE " & List3.Text & ";"
     DataContabil1.Database.Execute sSQL
  Next I
  DataContabil1.Refresh
  Unload FSeteos
End Sub

Private Sub Command19_Click()
' Cierre de CAJACREDITO
  RatonReloj
  FechaValida MBoxFechaC
  FechaFin = BuscarFecha(MBoxFechaC.Text)
  sSQL = "SELECT * FROM Cuentas " _
       & "ORDER BY Cuenta_No "
  SelectAdodc DataCaja, sSQL
  If DataCaja.Recordset.RecordCount > 0 Then
     FSeteos.Caption = "Eliminando Transacciones de Libretas "
     DataCaja.Recordset.MoveLast
     Cuenta_No = Mid(DataCaja.Recordset.Fields("Cuenta_No"), 1, 3)
   ' Eliminando flujo de libretas
     sSQL = "DELETE * FROM Trans_Libretas " _
          & "WHERE Fecha <= #" & FechaFin & "# " _
          & "AND Mid(Cuenta_No,1,3) <> '" & Cuenta_No & "' "
     DeleteData DataAuxCaja, sSQL
     FSeteos.Caption = "Eliminando Transacciones de Cajas "
   ' Eliminando flujo de caja
     sSQL = "DELETE * FROM Trans_Cajas " _
          & "WHERE Fecha <= #" & FechaFin & "# " _
          & "AND Mid(Cuenta_No,1,3) <> '" & Cuenta_No & "' " _
          & "AND TP <> 'BOVE' "
     DeleteData DataAuxCaja, sSQL
   ' Eliminando flujo de cajas diarias
     sSQL = "DELETE * FROM Trans_Cajas " _
          & "WHERE Fecha <= #" & FechaFin & "# " _
          & "AND Cuenta_No = '" & Ninguno & "' "
     DeleteData DataAuxCaja, sSQL
  End If
  FSeteos.Caption = "Eliminando Transacciones de Bancos"
' Eliminando depositos de bancos
  sSQL = "DELETE * FROM Bancos " _
       & "WHERE Fecha <= #" & FechaFin & "# " _
       & "AND T = 'N' " _
       & "AND Dias = 0 "
  DeleteData DataAuxCaja, sSQL
' Eliminando los depositos anulados
  sSQL = "DELETE * FROM Bancos " _
       & "WHERE T = 'A' "
  DeleteData DataAuxCaja, sSQL
  FSeteos.Caption = "Eliminando Transacciones de Encajes"
' Eliminando los Encajes anulados
  sSQL = "DELETE * FROM Bloqueos " _
       & "WHERE T = 'A' "
  DeleteData DataAuxCaja, sSQL
  FSeteos.Caption = "Eliminando Transacciones de Flujos de Cajas"
' Eliminando Saldo de Flujo de caja
  MiFecha = FechaFin
  sSQL = "SELECT * FROM Saldo_Caja_Libreta " _
       & "WHERE Fecha <= #" & FechaFin & "# " _
       & "ORDER BY Fecha DESC "
  SelectAdodc DataAuxCaja, sSQL
  If DataAuxCaja.Recordset.RecordCount > 0 Then
     FechaFin = BuscarFecha(DataAuxCaja.Recordset.Fields("Fecha"))
  End If
  sSQL = "DELETE * FROM Saldo_Caja_Libreta " _
       & "WHERE Fecha < #" & FechaFin & "# " _
       & "AND CL = True "
  DeleteData DataAuxCaja, sSQL
  FSeteos.Caption = "Eliminando Transacciones de Flujos de Libretas"
' Eliminando Saldo de Flujo de caja
  sSQL = "DELETE * FROM Saldo_Caja_Libreta " _
       & "WHERE Fecha < #" & FechaFin & "# " _
       & "AND CL = False "
  DeleteData DataAuxCaja, sSQL
  FSeteos.Caption = "Eliminando Saldo de Libretas"
' Eliminando Saldo de Libretas
  sSQL = "DELETE * FROM Saldo_Libretas " _
       & "WHERE Fecha < #" & FechaFin & "# "
  DeleteData DataAuxCaja, sSQL
  FSeteos.Caption = "Eliminando Transacciones de Prestamos"
' Eliminando los Prestamos
  FechaFin = MiFecha
  Contador = 0
  sSQL = "SELECT * FROM Prestamos " _
       & "WHERE Fecha <= #" & FechaFin & "# " _
       & "AND T = 'C' " _
       & "ORDER BY TP,Cuenta_No,Credito_No "
  SelectAdodc DataCaja, sSQL
  With DataCaja.Recordset
   If .RecordCount > 0 Then
       Do While Not .EOF
          Contador = Contador + 1
          TipoDoc = .Fields("TP")
          Numero = .Fields("Credito_No")
          Cuenta_No = .Fields("Cuenta_No")
          FSeteos.Caption = Contador & "/" & .RecordCount & ", Eliminando Transacciones de Prestamos " & TipoDoc & ", Credito No. " & Numero
          sSQL = "DELETE * FROM Trans_Prestamos " _
               & "WHERE Credito_No = " & Numero & " " _
               & "AND Cuenta_No = '" & Cuenta_No & "' " _
               & "AND TP = '" & TipoDoc & "' "
          DeleteData DataAuxCaja, sSQL
         .MoveNext
       Loop
   End If
  End With
  sSQL = "DELETE * FROM Prestamos " _
       & "WHERE Fecha <= #" & FechaFin & "# " _
       & "AND T = 'C' "
  DeleteData DataAuxCaja, sSQL
' Eliminar Movimientos antes del cierre
  Contador = 0
  sSQL = "SELECT * FROM Cuentas " _
       & "ORDER BY Cuenta_No "
  SelectAdodc DataCaja, sSQL
  With DataCaja.Recordset
   If .RecordCount > 0 Then
       Do While Not .EOF
          Contador = Contador + 1
          Cuenta_No = .Fields("Cuenta_No")
          FSeteos.Caption = Contador & "/" & .RecordCount & ", Eliminando Movimiento de Libreta No. " & Cuenta_No
        ' Eliminando Transacciones de libretas
        '  "ORDER BY Fecha,IDT,Hora,ID "
          sSQL = "SELECT * FROM Trans_Libretas " _
               & "WHERE Cuenta_No = '" & Cuenta_No & "' " _
               & "AND Fecha <= #" & FechaFin & "# " _
               & "ORDER BY Fecha DESC,IDT DESC,Hora DESC,ID DESC "
          SelectAdodc DataCajaCred, sSQL
          If DataCajaCred.Recordset.RecordCount > 0 Then
            'DataCajaCred.Recordset.MoveLast
             MiFecha = BuscarFecha(DataCajaCred.Recordset.Fields("Fecha"))
             sSQL = "DELETE * FROM Trans_Libretas " _
                  & "WHERE Cuenta_No = '" & Cuenta_No & "' " _
                  & "AND Fecha < #" & MiFecha & "# "
             DeleteData DataAuxCaja, sSQL
          End If
        ' Eliminando Transacciones de Cajas
          FSeteos.Caption = Contador & "/" & .RecordCount & ", Eliminando Movimiento de Cuenta No. " & Cuenta_No
          sSQL = "SELECT * FROM Trans_Cajas " _
               & "WHERE Cuenta_No = '" & Cuenta_No & "' " _
               & "AND Fecha <= #" & FechaFin & "# " _
               & "ORDER BY Fecha DESC,Hora DESC"
          SelectAdodc DataCajaCred, sSQL
          If DataCajaCred.Recordset.RecordCount > 0 Then
            'DataCajaCred.Recordset.MoveLast
             MiFecha = BuscarFecha(DataCajaCred.Recordset.Fields("Fecha"))
             sSQL = "DELETE * FROM Trans_Cajas " _
                  & "WHERE Cuenta_No = '" & Cuenta_No & "' " _
                  & "AND Fecha < #" & MiFecha & "# "
             DeleteData DataAuxCaja, sSQL
          End If
         .MoveNext
       Loop
   End If
  End With
  RatonNormal
  Unload FSeteos
End Sub

Private Sub Command2_Click()
   FechaValida MBoxFechaI, False
   FechaValida MBoxFechaF, False
   Titulo = "Pregunta de Actualizacion"
   Mensajes = "Seguro de Cambiar a Dorales el Periodo"
   If BoxMensaje = 6 Then
      RatonReloj
      FechaIni = BuscarFecha(MBoxFechaI.Text)
      FechaFin = BuscarFecha(MBoxFechaF.Text)
      sSQL = "SELECT * FROM Transacciones " _
           & "WHERE Fecha BETWEEN #" & FechaIni & "# and #" & FechaFin & "# "
      If TextTP.Text <> "" Then sSQL = sSQL & "AND TP = '" & TextTP.Text & "' "
      SelectAdodc DataContabil, sSQL
      With DataContabil.Recordset
       If .RecordCount > 0 Then
           RatonReloj
           Do While Not .EOF
             .Edit
             .Fields("Debe") = Round(.Fields("Debe") / Dolar)
             .Fields("Haber") = Round(.Fields("Haber") / Dolar)
             .Fields("Debe_ME") = 0
             .Fields("Haber_ME") = 0
             .Update
             .MoveNext
           Loop
       End If
      End With
      sSQL = "SELECT * FROM TransaccionesSC " _
           & "WHERE Fecha BETWEEN #" & FechaIni & "# and #" & FechaFin & "# "
      If TextTP.Text <> "" Then sSQL = sSQL & "AND TP = '" & TextTP.Text & "' "
      SelectAdodc DataContabil, sSQL
      With DataContabil.Recordset
       If .RecordCount > 0 Then
           RatonReloj
           Do While Not .EOF
             .Edit
             .Fields("Debitos") = Round(.Fields("Debitos") / Dolar)
             .Fields("Creditos") = Round(.Fields("Creditos") / Dolar)
             .Fields("Debitos_ME") = 0
             .Fields("Creditos_ME") = 0
             .Update
             .MoveNext
           Loop
       End If
      End With
      RatonNormal
   End If
   MsgBox "Proceso Terminado"
End Sub

Private Sub Command20_Click()
   FechaValida MBoxFechaI
   FechaValida MBoxFechaF
   Titulo = "Pregunta de Actualizacion"
   Mensajes = "Seguro de Actualizar Girtos Pagados"
   Contador = 1
   If BoxMensaje = 6 Then
      FechaIni = BuscarFecha(MBoxFechaI.Text)
      FechaFin = BuscarFecha(MBoxFechaF.Text)
      sSQL = "SELECT * " _
           & "FROM Correos " _
           & "WHERE Fecha_P BETWEEN #" & FechaIni & "# and #" & FechaFin & "# " _
           & "AND T = 'C' " _
           & "ORDER BY Fecha_P,Envio_No "
      SelectAdodc DataEnvios, sSQL
      sSQL = "SELECT * FROM Flujo_Caja "
      SelectAdodc DataResult, sSQL
      sSQL = "DELETE * FROM Flujo_Caja " _
           & "WHERE Fecha BETWEEN #" & FechaIni & "# and #" & FechaFin & "# "
      DeleteData DataResult, sSQL
      With DataEnvios.Recordset
       If .RecordCount > 0 Then
           RatonReloj
           Do While Not .EOF
              MiFecha = .Fields("Fecha_P")
              Codigo = .Fields("Envio_No")
              Codigo1 = .Fields("Sucursal")
              Codigo2 = .Fields("Usuario")
              Total = .Fields("TOTAL")
              DataResult.Recordset.AddNew
              DataResult.Recordset.Fields("Envio_No") = Codigo
              DataResult.Recordset.Fields("Fecha") = MiFecha
              DataResult.Recordset.Fields("Sucursal") = Codigo1
              DataResult.Recordset.Fields("Egreso_MN") = Total
              DataResult.Recordset.Fields("Usuario") = Codigo2
              DataResult.Recordset.Update
              FSeteos.Caption = Codigo & ": " & Contador & "/" & .RecordCount
              Contador = Contador + 1
             .MoveNext
           Loop
        End If
      End With
   End If
   RatonNormal
End Sub

Private Sub Command3_Click()
  If ClaveSupervisor Then
     Titulo = "Pregunta de Procesos"
     Mensajes = "Seguro de Reprocesar" & vbCrLf _
              & "todos los Comprobantes"
     If BoxMensaje = 6 Then
        sSQL = "SELECT * FROM Comprobantes " _
             & "ORDER BY Item,Tp,Numero "
        SelectDBGrid DBGrid1, DataContabil, sSQL
        RatonReloj
        Contador = 1
        DBGrid1.Visible = False
        With DataContabil.Recordset
         If .RecordCount > 0 Then
             Do While Not .EOF
               'Listar Comprobante
                NumItemTemp = .Fields("Item")
                FechaTexto = .Fields("Fecha")
                Numero = .Fields("Numero")
                TipoComp = .Fields("TP")
                TipoDoc = .Fields("T")
                FSeteos.Caption = TipoComp & "-" & Numero & ", " & Contador & "/" & .RecordCount
                IniciarAsientosDataDe DataAsientos, DataSubCtaDet, DataAsientBanco, DataDetRet, DataIngArt
                ListarComprobante TipoComp, NumItemTemp, Numero, DataAsientos, DataAsientBanco, DataSubCtaDet, DataDetRet
                If DataAsientos.Recordset.RecordCount > 0 Then
                   DataAsientos.Recordset.MoveFirst
                   Do While Not DataAsientos.Recordset.EOF
                      Codigo = DataAsientos.Recordset.Fields("CODIGO")
                      If DataAsientos.Recordset.Fields("DEBE") > 0 Then OpcDH = "1"
                      If DataAsientos.Recordset.Fields("HABER") > 0 Then OpcDH = "2"
                      If DataSubCtaDet.Recordset.RecordCount > 0 Then
                         DataSubCtaDet.Recordset.MoveFirst
                         Do While Not DataSubCtaDet.Recordset.EOF
                            DataSubCtaDet.Recordset.Edit
                            If Codigo = DataSubCtaDet.Recordset.Fields("Cta") Then
                               DataSubCtaDet.Recordset.Fields("DH") = OpcDH
                               DataSubCtaDet.Recordset.Update
                            End If
                            DataSubCtaDet.Recordset.MoveNext
                         Loop
                      End If
                      DataAsientos.Recordset.MoveNext
                   Loop
                End If
               'Grabacion del Comp
                Co.T = TipoDoc
                Co.Item = NumItemTemp
                Co.TP = TipoComp
                Co.Fecha = FechaTexto
                Co.Numero = Numero
                GrabarComprobante Co, DataAsientos, DataSubCtaDet, DataAsientBanco, DataDetRet
                Contador = Contador + 1
               .MoveNext
             Loop
         End If
        End With
        DBGrid1.Visible = True
        RatonNormal
     End If
  End If
End Sub

Private Sub Command4_Click()
   RatonReloj
   DataContabil.Database.Close
   DataProducc.Database.Close
   DataFacturas.Database.Close
   DataFactTurs.Database.Close
   DataCodigos.Database.Close
   DataCtasProc.Database.Close
   DataSubCtas.Database.Close
   DataCorreos.Database.Close
   DataGanadero.Database.Close
   For Ind = 0 To File1.ListCount - 1
       Cadena = Dir1.Path & "\" & File1.List(Ind)
       CompactarDatabase Cadena, False
   Next Ind
   RatonNormal
   Unload FSeteos
End Sub

Private Sub Command5_Click()
   FechaValida MBoxFechaI, False
   FechaValida MBoxFechaF, False
   If ClaveSupervisor Then
      Titulo = "Pregunta de Eliminar"
      Mensajes = "Seguro de Borrar el Periodo"
      If BoxMensaje = 6 Then
         RatonReloj
         FechaIni = BuscarFecha(MBoxFechaI.Text)
         FechaFin = BuscarFecha(MBoxFechaF.Text)
         sSQL = "DELETE * FROM Balance_General "
         DeleteData DataContabil, sSQL
         sSQL = "DELETE * FROM Balance_Consolidado "
         DeleteData DataContabil, sSQL
         sSQL = "DELETE * FROM Estado_Resultado "
         DeleteData DataContabil, sSQL
         sSQL = "DELETE * FROM Estado_Resultado_Mes "
         DeleteData DataContabil, sSQL
         sSQL = "DELETE * FROM Estado_Resultado_12Meses "
         DeleteData DataContabil, sSQL
         sSQL = "DELETE * FROM Transacciones " _
              & "WHERE Fecha BETWEEN #" & FechaIni & "# and #" & FechaFin & "# "
         DeleteData DataContabil, sSQL
         sSQL = "DELETE * FROM TransaccionesSC " _
              & "WHERE Fecha BETWEEN #" & FechaIni & "# and #" & FechaFin & "# "
         DeleteData DataContabil, sSQL
         sSQL = "DELETE * FROM Comprobantes " _
              & "WHERE Fecha BETWEEN #" & FechaIni & "# and #" & FechaFin & "# "
         DeleteData DataContabil, sSQL
         sSQL = "DELETE * FROM Bancos " _
              & "WHERE Fecha BETWEEN #" & FechaIni & "# and #" & FechaFin & "# "
         DeleteData DataContabil, sSQL
         sSQL = "DELETE * FROM Retenciones " _
              & "WHERE Fecha BETWEEN #" & FechaIni & "# and #" & FechaFin & "# "
         DeleteData DataContabil, sSQL
         RatonNormal
      End If
      MsgBox "Proceso Terminado"
   End If
End Sub

Private Sub Command6_Click()
sSQL = "SELECT * FROM Comprobantes " _
     & "ORDER BY Item,TP,Numero,Fecha "
SelectAdodc DataContabil, sSQL
List1.Clear
List1.AddItem "Comprobantes Incompletos Borrados:"
Contador = 0
With DataContabil.Recordset
 If .RecordCount > 0 Then
     Do While Not .EOF
     FSeteos.Caption = Contador & " de " & .RecordCount
     Item = .Fields("Item")
     Numero = .Fields("Numero")
     TipoProc = .Fields("TP")
     MiFecha = .Fields("Fecha")
     sSQL = "SELECT * FROM Transacciones " _
          & "WHERE Item = " & Item & " " _
          & "AND TP = '" & TipoProc & "' " _
          & "AND Numero = " & Numero & " "
     SelectAdodc DataConta, sSQL
     If DataConta.Recordset.RecordCount <= 0 Then
        List1.AddItem MiFecha & " - " & Item & "-" & TipoProc & "-" & Numero
        sSQL = "DELETE * FROM Comprobantes " _
             & "WHERE Item = " & Item & " " _
             & "AND TP = '" & TipoProc & "' " _
             & "AND Numero = " & Numero & " " _
             & "AND Fecha = #" & BuscarFecha(MiFecha) & "# "
        DeleteData DataSaldoSC, sSQL
     End If
     Contador = Contador + 1
    .MoveNext
     Loop
 End If
End With
sSQL = "SELECT Item,Fecha,TP,Numero,SUM(Debe) As TDebe,SUM(Haber) As THaber " _
     & "FROM Transacciones " _
     & "GROUP BY Item,TP,Numero,Fecha "
SelectAdodc DataContabil, sSQL
Contador = 0
With DataContabil.Recordset
 If .RecordCount > 0 Then
     Do While Not .EOF
     FSeteos.Caption = Contador & " de " & .RecordCount
     Item = .Fields("Item")
     Numero = .Fields("Numero")
     TipoProc = .Fields("TP")
     MiFecha = .Fields("Fecha")
     sSQL = "SELECT * FROM Comprobantes " _
          & "WHERE Item = " & Item & " " _
          & "AND TP = '" & TipoProc & "' " _
          & "AND Numero = " & Numero & " "
     SelectAdodc DataConta, sSQL
     If DataConta.Recordset.RecordCount <= 0 Then
        List1.AddItem MiFecha & " - " & Item & "-" & TipoProc & "-" & Numero
        sSQL = "DELETE * FROM Transacciones " _
             & "WHERE Item = " & Item & " " _
             & "AND TP = '" & TipoProc & "' " _
             & "AND Numero = " & Numero & " " _
             & "AND Fecha = #" & BuscarFecha(MiFecha) & "# "
        DeleteData DataSaldoSC, sSQL
     End If
     Contador = Contador + 1
    .MoveNext
     Loop
 End If
End With
End Sub

Private Sub Command7_Click()
Dim Td As TableDef
Dim Encontro As Boolean
  'DataContabil1.Database.Close
  List2.Clear
  List3.Clear
  Cadena = "Bases: " & vbCrLf
  For Each Td In DataContabil1.Database.TableDefs
   If DataEmpresa.Recordset.RecordCount > 0 Then
      DataEmpresa.Recordset.MoveFirst
      Encontro = True
      Do While Not DataEmpresa.Recordset.EOF
         Cadena1 = SinEspaciosIzq(DataEmpresa.Recordset.Fields("NombUsuar"))
         Cadena2 = Right(Td.Name, Len(Cadena1))
         If Cadena1 = Cadena2 Then
            List3.AddItem Td.Name
            Encontro = False
         End If
         DataEmpresa.Recordset.MoveNext
      Loop
      If Encontro And UCase(Mid(Td.Name, 1, 4)) <> "MSYS" Then List2.AddItem Td.Name
   End If
  Next
End Sub

Private Sub Command8_Click()
  sSQL = "SELECT * FROM Costos_R_" & CodigoUsuario
  SelectAdodc DataConta, sSQL
  sSQL = "DELETE * FROM Costos_R_" & CodigoUsuario
  DeleteData DataConta, sSQL
  sSQL = "UPDATE Transacciones As T,Bancos As B " _
       & "SET T.Cheq_Dep = B.Cheq_Dep " _
       & "WHERE T.Cta = B.Cta_Banco " _
       & "AND  T.TP = B.TP " _
       & "AND  T.Numero = B.Numero " _
       & "AND  T.Cheq_Dep = '" & Ninguno & "' "
  UpdateData DataContabil, sSQL
  sSQL = "UPDATE Comprobantes SET Concepto = 'C O M P R O B A N T E    A N U L A D O' " _
       & "WHERE T = 'A' "
  UpdateData DataContabil, sSQL
  sSQL = "UPDATE Transacciones SET " _
       & "Debe = 0,Haber = 0,Debe_ME = 0,Haber_ME = 0," _
       & "Saldo = 0,Saldo_ME = 0 " _
       & "WHERE T = 'A' "
  UpdateData DataContabil, sSQL
  sSQL = "UPDATE TransaccionesSC SET " _
       & "Debitos = 0,Creditos = 0,Debitos_ME = 0,Creditos_ME = 0," _
       & "Saldo = 0,Saldo_ME = 0 " _
       & "WHERE T = 'A' "
  UpdateData DataContabil, sSQL
  
  sSQL = "SELECT * FROM Transacciones " _
       & "ORDER BY TP,Numero,Fecha,Item "
  SelectAdodc DataContabil, sSQL, False
  RatonReloj
  With DataContabil.Recordset
   If .RecordCount > 0 Then
      .MoveFirst
       Cadena = "Comprobantes:" & vbCrLf
       Debe = 0: Haber = 0
       NumItem = .Fields("Item")
       Cta = .Fields("Cta")
       MiFecha = .Fields("Fecha")
       TipoComp = .Fields("TP")
       Numero = .Fields("Numero")
       Do While Not .EOF
          If TipoComp <> .Fields("TP") Or Numero <> .Fields("Numero") Or NumItem <> .Fields("Item") Then
             FSeteos.Caption = MiFecha & " TP: " & TipoComp & " No. " & Numero
             If Round(Debe - Haber) <> 0 Then
                Cadena = Cadena & " TP: " & TipoComp & " No. " & Numero & " -> " & Round(Debe - Haber) & vbCrLf
                DataConta.Recordset.AddNew
                DataConta.Recordset.Fields("Fecha") = MiFecha
                DataConta.Recordset.Fields("Beneficiario") = Ninguno
                DataConta.Recordset.Fields("Cuenta") = Cta
                DataConta.Recordset.Fields("TP") = TipoComp
                DataConta.Recordset.Fields("Numero") = Numero
                DataConta.Recordset.Fields("Debitos") = Debe
                DataConta.Recordset.Fields("Creditos") = Haber
                DataConta.Recordset.Fields("Valor_ME") = NumItem
                DataConta.Recordset.Update
             End If
             Debe = 0: Haber = 0
             NumItem = .Fields("Item")
             TipoComp = .Fields("TP")
             Numero = .Fields("Numero")
             Cta = .Fields("Cta")
             MiFecha = .Fields("Fecha")
          End If
          Debe = Debe + .Fields("Debe")
          Haber = Haber + .Fields("Haber")
         .MoveNext
       Loop
   End If
  End With
  If Round(Debe - Haber) <> 0 Then
     DataConta.Recordset.AddNew
     DataConta.Recordset.Fields("Fecha") = MiFecha
     DataConta.Recordset.Fields("Beneficiario") = Ninguno
     DataConta.Recordset.Fields("Cuenta") = Cta
     DataConta.Recordset.Fields("TP") = TipoComp
     DataConta.Recordset.Fields("Numero") = Numero
     DataConta.Recordset.Fields("Debitos") = Debe
     DataConta.Recordset.Fields("Creditos") = Haber
     DataConta.Recordset.Fields("Valor_ME") = NumItem
     DataConta.Recordset.Update
  End If
  sSQL = "SELECT TP,Numero,Fecha,Item " _
       & "FROM Transacciones " _
       & "GROUP BY TP,Numero,Fecha,Item "
  SelectAdodc DataContabil, sSQL, False
  RatonReloj
  With DataContabil.Recordset
   If .RecordCount > 0 Then
      .MoveFirst
       Do While Not .EOF
          NumItem = .Fields("Item")
          MiFecha = .Fields("Fecha")
          TipoComp = .Fields("TP")
          Numero = .Fields("Numero")
          FSeteos.Caption = "G: " & MiFecha & " TP: " & TipoComp & " No. " & Numero
          sSQL = "SELECT * FROM Comprobantes " _
               & "WHERE TP='" & TipoComp & "' " _
               & "AND Numero=" & Numero & " " _
               & "AND Fecha=# " & BuscarFecha(MiFecha) & "# " _
               & "AND Item=" & NumItem & " "
          SelectAdodc DataSaldoSC, sSQL
          If DataSaldoSC.Recordset.RecordCount <= 0 Then
             DataConta.Recordset.AddNew
             DataConta.Recordset.Fields("Fecha") = MiFecha
             DataConta.Recordset.Fields("Beneficiario") = Ninguno
             DataConta.Recordset.Fields("Cuenta") = Ninguno
             DataConta.Recordset.Fields("TP") = TipoComp
             DataConta.Recordset.Fields("Numero") = Numero
             DataConta.Recordset.Fields("Debitos") = 0
             DataConta.Recordset.Fields("Creditos") = 0
             DataConta.Recordset.Fields("Valor_ME") = NumItem
             DataConta.Recordset.Update
          End If
         .MoveNext
       Loop
   End If
  End With
  sSQL = "SELECT * FROM Costos_R_" & CodigoUsuario
  SelectAdodc DataContabil, sSQL
  RatonNormal
  FSeteos.Caption = "SETEOS GENERALES DE MANTENIMIENTO"
  MsgBox Cadena
End Sub

Private Sub Command9_Click()
  Cadena = DBGrid2.Columns(I).DataField
  sSQL = "SELECT * FROM " & List2.Text & " " _
       & "ORDER BY " & Cadena & " DESC "
  SelectAdodc DataContabil1, sSQL
End Sub

Private Sub DBCListEmp_KeyDown(KeyCode As Integer, Shift As Integer)
  Keys_Especiales Shift
  If KeyCode = vbKeyInsert Then
     Codigo = SinEspaciosIzq(DBCUsuario.Text)
     sSQL = "SELECT * FROM Acceso_Empresa " _
          & "WHERE Codigo = '" & Codigo & "' "
     SelectDBGrid DBGEmp, DataBuscEmp, sSQL
     sSQL = "SELECT * FROM Empresas " _
          & "WHERE Empresa = '" & DBCListEmp.Text & "' "
     SelectAdodc DataEmpresa1, sSQL
     With DataBuscEmp.Recordset
       If DataEmpresa1.Recordset.RecordCount > 0 Then
         .AddNew
         .Fields("Codigo") = Codigo
         .Fields("Empresa") = DataEmpresa1.Recordset.Fields("Empresa")
         .Fields("SubDir") = DataEmpresa1.Recordset.Fields("SubDir")
         .Update
       End If
     End With
     sSQL = "SELECT * FROM Acceso_Empresa " _
          & "WHERE Codigo = '" & Codigo & "' " _
          & "ORDER BY Empresa "
     SelectDBGrid DBGEmp, DataBuscEmp, sSQL
  End If
  If CtrlDown And KeyCode = vbKeyInsert Then
     Codigo = SinEspaciosIzq(DBCUsuario.Text)
     sSQL = "SELECT * FROM Acceso_Empresa "
     SelectDBGrid DBGEmp, DataBuscEmp, sSQL
     sSQL = "SELECT * FROM Empresas " _
          & "ORDER BY Empresa "
     SelectAdodc DataEmpresa1, sSQL
     With DataBuscEmp.Recordset
       If DataEmpresa1.Recordset.RecordCount > 0 Then
          DataEmpresa1.Recordset.MoveFirst
          Do While Not DataEmpresa1.Recordset.EOF
            .AddNew
            .Fields("Codigo") = Codigo
            .Fields("Empresa") = DataEmpresa1.Recordset.Fields("Empresa")
            .Fields("SubDir") = DataEmpresa1.Recordset.Fields("SubDir")
            .Update
             DataEmpresa1.Recordset.MoveNext
          Loop
       End If
     End With
     sSQL = "SELECT * FROM Acceso_Empresa " _
          & "WHERE Codigo = '" & Codigo & "' " _
          & "ORDER BY Empresa "
     SelectDBGrid DBGEmp, DataBuscEmp, sSQL
  End If
End Sub

Private Sub DBCUsuario_KeyDown(KeyCode As Integer, Shift As Integer)
  PresionoEnter KeyCode
End Sub

Private Sub DBCUsuario_LostFocus()
   For I = 0 To 5
      Nivel_N(I) = Val(True)
   Next I
   CSupervisor = Val(False)
   Codigo = SinEspaciosIzq(DBCUsuario.Text)
   sSQL = "SELECT * FROM Accesos " _
        & "WHERE Codigo = '" & Codigo & "' "
   SelectAdodc DataEmpresa1, sSQL, False
   With DataEmpresa1.Recordset
    If .RecordCount > 0 Then TextClave.Text = .Fields("Clave") Else TextClave.Text = ""
   End With
   sSQL = "SELECT * FROM " & UCase(App.EXEName) & " " _
        & "WHERE Codigo = '" & Codigo & "' "
   SelectAdodc DataEmpresa1, sSQL, False
   With DataEmpresa1.Recordset
    If .RecordCount > 0 Then
        Nivel_N(0) = .Fields("Nivel_1")
        Nivel_N(1) = .Fields("Nivel_2")
        Nivel_N(2) = .Fields("Nivel_3")
        Nivel_N(3) = .Fields("Nivel_4")
        Nivel_N(4) = .Fields("Nivel_5")
        Nivel_N(5) = .Fields("Nivel_6")
        CSupervisor = .Fields("Supervisor")
        For I = 0 To 5
           If Nivel_N(I) <> 0 Then Nivel_N(I) = 1
        Next I
        If CSupervisor <> 0 Then CSupervisor = 1
    Else
        MsgBox "Este usuario aun no esta seteado"
    End If
   End With
   sSQL = "SELECT * FROM Acceso_Empresa " _
        & "WHERE Codigo = '" & Codigo & "' " _
        & "ORDER BY Empresa "
   SelectDBGrid DBGEmp, DataBuscEmp, sSQL
   For I = 0 To 5
       CheckNivel(I).Value = Nivel_N(I)
   Next I
   CheckSupervisor.Value = CSupervisor
   CheckNivel(0).SetFocus
End Sub

Private Sub DBGrid1_KeyDown(KeyCode As Integer, Shift As Integer)
   If KeyCode = vbKeyF1 Then GenerarDataTexto FSeteos, DataContabil
End Sub

