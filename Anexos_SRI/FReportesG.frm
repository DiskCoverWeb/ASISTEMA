Attribute VB_Name = "FReportesG"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

Private Sub CmdPresentar_Click()
    If OptBienes.Value = True Then
       Porcentajes_Bienes
    Else
       Porcentajes_Servicios
    End If
End Sub

Public Sub Porcentajes_Bienes()
    sSQL = "SELECT ('Retención de Bienes') As Concepto, COUNT(Porc_Bienes) As Cant,SUM(ValorRetBienes) As ValorRetBienes,SUM(BaseImponible) As BaseImp " _
         & "FROM Trans_Compras " _
         & "WHERE Item = '" & NumEmpresa & "' " _
         & "AND Porc_Bienes <> '.'  " _
         & "AND Periodo = '" & Periodo_Contable & "' "
    SelectDataGrid DGBienes, AdoComprasBienes, sSQL
    
    sSQL = "SELECT ('Retención de Bienes') As Concepto, COUNT(Porc_Bienes) As Cant,SUM(ValorRetBienes) As ValorRetBienes,SUM(BaseImponible) As BaseImp " _
         & "FROM Trans_Ventas " _
         & "WHERE Item = '" & NumEmpresa & "' " _
         & "AND Porc_Bienes <> '.'  " _
         & "AND Periodo = '" & Periodo_Contable & "' "
    SelectDataGrid DGVentasBienes, AdoVentasBienes, sSQL
    
    DGBienes.Visible = True
    DGServicios.Visible = False
    DGVentasBienes.Visible = True
    DGVentasServicios.Visible = False
    
'    DGimportacionesBienes.Visible = True
'    DGimportacionesServicios.Visible = False
'    DGexportacionesBienes.Visible = True
'    DGexportacionesServicios.Visible = False
End Sub

Public Sub Porcentajes_Servicios()
    sSQL = "SELECT ('Retención de Servicios') As Concepto, COUNT(Porc_Servicios) As Cant,SUM(ValorRetServicios) As ValorRetServicios,SUM(BaseImponible) As BaseImp " _
         & "FROM Trans_Compras " _
         & "WHERE Item = '" & NumEmpresa & "' " _
         & "AND Porc_Servicios <> '.'  " _
         & "AND Periodo = '" & Periodo_Contable & "' "
    SelectDataGrid DGServicios, AdoComprasServicios, sSQL
    
    sSQL = "SELECT ('Retención de Servicios') As Concepto, COUNT(Porc_Servicios) As Cant,SUM(ValorRetServicios) As ValorRetServicios,SUM(BaseImponible) As BaseImp " _
         & "FROM Trans_Ventas " _
         & "WHERE Item = '" & NumEmpresa & "' " _
         & "AND Porc_Servicios <> '.'  " _
         & "AND Periodo = '" & Periodo_Contable & "' "
    SelectDataGrid DGVentasServicios, AdoVentasServicios, sSQL
    DGServicios.Visible = True
    DGBienes.Visible = False
    DGVentasServicios.Visible = True
    DGVentasBienes.Visible = False
    
    
End Sub

Private Sub CmdSalir_Click()
    Unload Me
End Sub

Private Sub Form_Activate()
    OptBienes.SetFocus
End Sub

Private Sub Form_Load()
    ConectarAdodc AdoComprasBienes
    ConectarAdodc AdoComprasServicios
    ConectarAdodc AdoVentasBienes
    ConectarAdodc AdoVentasServicios
    ConectarAdodc AdoImportaciones
    ConectarAdodc AdoExportaciones
    ConectarAdodc AdoAirC
    ConectarAdodc AdoAirV
    ConectarAdodc AdoAirI
    ConectarAdodc AdoQuery
End Sub
