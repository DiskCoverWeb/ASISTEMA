Option Explicit

Private Sub Command1_Click()
  FechaValida MBDesde
  FechaValida MBHasta
  FechaIni = BuscarFecha(MBDesde)
  FechaFin = BuscarFecha(MBHasta)
  Cta = SinEspaciosIzq(DCCtas)
  If Cta = "" Then Cta = Ninguno
  sSQL = "SELECT Fecha,TP,Numero,Factura,Prima,Debitos,Creditos,Cta,Item,Periodo,ID " _
       & "FROM Trans_SubCtas " _
       & "WHERE Fecha BETWEEN #" & FechaIni & "# and #" & FechaFin & "# " _
       & "AND TC = 'PM' " _
       & "AND Cta = '" & Cta & "' " _
       & "AND Item = '" & NumEmpresa & "' " _
       & "AND Periodo = '" & Periodo_Contable & "' " _
       & "ORDER BY Cta,Fecha,TP,Numero "
  Select_Adodc_Grid DGPrimas, AdoTrans, sSQL
End Sub

Private Sub Command2_Click()
  MensajeEncabData = "LISTADO DE PRIMAS"
  ImprimirAdodc AdoTrans, 2, 8
End Sub

Private Sub Command3_Click()
  Unload FPrimas
End Sub

Private Sub Form_Activate()
  sSQL = "SELECT Codigo & Space(20) & Cuenta As Nombre_Cta " _
       & "FROM Catalogo_Cuentas " _
       & "WHERE TC = 'PM' " _
       & "AND DG = 'D' " _
       & "AND Item = '" & NumEmpresa & "' " _
       & "AND Periodo = '" & Periodo_Contable & "' " _
       & "ORDER BY Codigo "
  SelectDB_Combo DCCtas, AdoCtas, sSQL, "Nombre_Cta", False
  RatonNormal
End Sub

Private Sub Form_Load()
  ConectarAdodc AdoCtas
  ConectarAdodc AdoTrans
End Sub

